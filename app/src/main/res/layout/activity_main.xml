<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#836D4A"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="8dp"
        android:background="#836D4A"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/TK_RaceSymbol" />

    <Button
        android:id="@+id/button"
        android:layout_width="139dp"
        android:layout_height="76dp"
        android:layout_margin="20dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="5dp"
        android:text="Useless Button"
        android:textColor="#FFFAFA"
        app:backgroundTint="#B81C1C"
        app:layout_constraintBottom_toTopOf="@+id/checkBox"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:id="@+id/button2"
        android:layout_width="144dp"
        android:layout_height="75dp"
        android:layout_margin="20dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="8dp"
        android:text="Useless Button"
        android:textColor="#F8F8F8"
        app:backgroundTint="#B31C1C"
        app:layout_constraintBottom_toTopOf="@+id/checkBox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="302dp"
        android:layout_height="72dp"
        android:layout_marginStart="57dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="170dp"
        android:text="I Promise to sell my soul to the Tomb Kings of Nehekara  and give my life for them"
        android:textAlignment="center"
        android:textSize="12sp"
        app:buttonTint="#F8F8F8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="101dp"
        android:layout_height="158dp"
        android:layout_margin="10dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toStartOf="@+id/checkBox3"
        tools:layout_editor_absoluteY="569dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="62dp"
        android:layout_height="102dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox2"
        app:layout_constraintTop_toTopOf="@+id/checkBox2"
        app:srcCompat="@drawable/Court_of_Lybaras"
        tools:layout_editor_absoluteX="32dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="94dp"
        android:layout_height="154dp"
        android:layout_margin="10dp"
        app:layout_constraintEnd_toStartOf="@+id/checkBox4"
        tools:layout_editor_absoluteY="569dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="61dp"
        android:layout_height="87dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox3"
        app:layout_constraintTop_toTopOf="@+id/checkBox3"
        app:srcCompat="@drawable/Khemri"
        tools:layout_editor_absoluteX="133dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="85dp"
        android:layout_height="158dp"
        android:layout_margin="10dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/checkBox5"
        tools:layout_editor_absoluteY="569dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="61dp"
        android:layout_height="84dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox4"
        app:layout_constraintTop_toTopOf="@+id/checkBox4"
        app:layout_constraintVertical_bias="0.486"
        app:srcCompat="@drawable/Followers_of_Nagash"
        tools:layout_editor_absoluteX="237dp" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="94dp"
        android:layout_height="164dp"
        android:layout_margin="10dp"
        tools:layout_editor_absoluteX="308dp"
        tools:layout_editor_absoluteY="566dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="60dp"
        android:layout_height="87dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox5"
        app:layout_constraintTop_toTopOf="@+id/checkBox5"
        app:srcCompat="@drawable/Exiles_of_Nehek"
        tools:layout_editor_absoluteX="342dp" />

</androidx.constraintlayout.widget.ConstraintLayout>